	}

	void addQuestion(String content, String varA, String varB, String varC){
		Question q = new Question();
		
	}
	
	void addEmptyQuestion(){
		Question q = new Question();
	}
	
	void removeQuestion(int i){
		questions.remove(i);
	}
	
}
