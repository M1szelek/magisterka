import java.awt.Image;


public class Question {
	private String content;
	private QuestionVariant varA;
	private QuestionVariant varB;
	private QuestionVariant varC;
	private int correct;
	
	private Image imgContent;
	private Image imgVarA;
	private Image imgVarB;
	private Image imgVarC;
	
	public Question() {
		super();
		this.content = new String();
		
	}
	
	public Image getImgContent() {
		return imgContent;
	}
	public void setImgContent(Image imgContent) {
		this.imgContent = imgContent;
	}
	public Image getImgVarA() {
		return imgVarA;
	}
	public void setImgVarA(Image imgVarA) {
		this.imgVarA = imgVarA;
	}
	public Image getImgVarB() {
		return imgVarB;
	}
	
	public void setImgVarB(Image imgVarB) {
		this.imgVarB = imgVarB;
	}
	public Image getImgVarC() {
		return imgVarC;
	}
	public void setImgVarC(Image imgVarC) {
		this.imgVarC = imgVarC;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public QuestionVariant getVariantA() {
		return varA;
	}
	public void setVariantA(QuestionVariant variantA) {
		this.varA = variantA;
	}
	public QuestionVariant getVariantB() {
		return varB;
	}
	public void setVariantB(QuestionVariant variantB) {
		this.varB = variantB;
	}
	public QuestionVariant getVariantC() {
		return varC;
	}
	public void setVariantC(QuestionVariant variantC) {
		this.varC = variantC;
	}
	
	
	
	
	
}
