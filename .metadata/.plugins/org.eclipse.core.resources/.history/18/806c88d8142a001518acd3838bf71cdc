	public QBase(String name, String author) {
		super();
		this.name = name;	
		this.author = author;
		questions = new ArrayList<Question>();
	}



	void addQuestion(){
		
	}
	
	void removeQuestion(int i){
		questions.remove(i);
	}
	
}
